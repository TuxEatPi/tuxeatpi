      Parles tu
      <annotation conceptref="language">
        anglais
      </annotation>
    </sample>
    <sample intentref="aptitudes__being__set_lang">
      Do you understand
      <annotation conceptref="language">
        french
      </annotation>
    </sample>
    <sample intentref="aptitudes__being__set_lang">
